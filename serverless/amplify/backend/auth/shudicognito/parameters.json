{
    "identityPoolName": "shudi_identitypool",
    "allowUnauthenticatedIdentities": true,
    "lambdaLogPolicy": "shudicognito_lambda_log_policy",
    "openIdLambdaRoleName": "shudicognito_openid_lambda_role",
    "openIdRolePolicy": "shudicognito_openid_pass_role_policy",
    "openIdLambdaIAMPolicy": "shudicognito_openid_lambda_iam_policy",
    "openIdLogPolicy": "shudicognito_openid_lambda_log_policy",
    "userPoolName": "shudi_userpool",
    "autoVerifiedAttributes": [
        "email"
    ],
    "mfaConfiguration": "OPTIONAL",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "shudicognito_sns-role",
    "roleExternalId": "shudicognito_role_external_id",
    "policyName": "shudicognito-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": false,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
        "Requires Lowercase",
        "Requires Uppercase",
        "Requires Numbers",
        "Requires Symbols"
    ],
    "requiredAttributes": [
        "email",
        "name"
    ],
    "userpoolClientName": "shudicognito_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": [
        "phone_number",
        "email",
        "name"
    ],
    "userpoolClientReadAttributes": [
        "email",
        "name",
        "phone_number",
        "email_verified",
        "phone_number_verified"
    ],
    "mfaLambdaRole": "shudicognito_totp_lambda_role",
    "mfaLambdaLogPolicy": "shudicognito_totp_lambda_log_policy",
    "mfaPassRolePolicy": "shudicognito_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "shudicognito_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "shudicognito_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "shudicognito_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "shudicognito_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": true,
    "useDefault": "manual",
    "authSelections": "identityPoolAndUserPool",
    "resourceName": "shudicognito",
    "thirdPartyAuth": false,
    "usernameAttributes": [
        "email, phone_number"
    ],
    "hostedUI": false,
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}